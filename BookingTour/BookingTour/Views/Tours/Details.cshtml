        <script>
            async function submitBooking(event) {
                event.preventDefault();

                const tourId = document.querySelector("input[name='tourId']").value;
                const departureDate = document.getElementById("departureDate").value;
                const quantityAdult = document.getElementById("quantityAdult").value;
                const quantityChild = document.getElementById("quantityChild").value;
                const quantityInfant = document.getElementById("quantityInfant").value;
                const totalPrice = document.getElementById("totalPrice").value;

                const bookingData = {
                    TourId: parseInt(tourId),
                    DepartureDate: departureDate,
                    QuantityAdult: parseInt(quantityAdult),
                    QuantityChild: parseInt(quantityChild),
                    QuantityInfant: parseInt(quantityInfant),
                    TotalPrice: parseFloat(totalPrice)
                };

                try {
                    const response = await fetch('/api/Booking', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify(bookingData)
                    });

                    if (!response.ok) {
                        throw new Error("Lỗi khi đặt tour");
                    }

                    const result = await response.json();

                    if (!result.invoiceId) {
                        throw new Error("Không lấy được ID hóa đơn");
                    }

                    // ✅ Chuyển hướng đến trang InvoiceDetails
                    window.location.href = `/Tours/Detaillnvoice?id=${result.invoiceId}`;



                } catch (error) {
                    alert("Đặt tour thất bại, vui lòng thử lại!");
                    console.error("Lỗi:", error);
                }
            }

            document.getElementById("bookingForm").addEventListener("submit", submitBooking);

        </script>
        <script>
                        document.addEventListener("DOMContentLoaded", function () {
                fetchDepartureDates();
            });

            async function fetchDepartureDates() {
                const tourId = @Model.IdTour;
                console.log(tourId);
                const apiUrl = `https://localhost:7263/api/TourDetailAPI/tour/${tourId}`;
                try {
                    const response = await fetch(apiUrl);
                    const data = await response.json();
                    console.log(data);

                    // Lấy ngày hiện tại (UTC để đồng bộ với API)
                    const today = new Date();
                    today.setUTCHours(0, 0, 0, 0); // Đặt giờ về 00:00 UTC để tránh lỗi so sánh

                    // Lọc chỉ lấy ngày khởi hành sau ngày hiện tại và sắp xếp theo thứ tự gần nhất đến xa nhất
                    const departureDates = data
                        .map(item => item.date)
                        .filter(date => date && new Date(date) > today)
                        .sort((a, b) => new Date(a) - new Date(b)); // Sắp xếp tăng dần (gần trước, xa sau)

                    updateDepartureOptions(departureDates);
                } catch (error) {
                    console.error("Error fetching departure dates:", error);
                }
            }

            function updateDepartureOptions(dates) {
                const departureOptions = document.getElementById("departureOptions");
                const departureDateInput = document.getElementById("departureDate");
                const dropdown = document.getElementById("customDepartureDate");
                const dropdownContainer = document.getElementById("departureDropdownContainer");
                const departureContainer = document.getElementById("departureContainer");
                const departureLabel = document.getElementById("departureLabel");

                departureOptions.innerHTML = "";
                dropdown.innerHTML = "";

                if (dates.length === 0) {
                    dropdownContainer.style.display = "none";
                    departureContainer.style.display = "none";
                    departureLabel.style.display = "none";
                    return;
                } else {
                    dropdownContainer.style.display = "block";
                    departureContainer.style.display = "block";
                    departureLabel.style.display = "block";
                }

                dates.forEach((isoDate, index) => {
                    let formattedDate = formatDateToDDMM(isoDate);

                    let button = document.createElement("button");
                    button.className = "btn btn-outline-primary rounded";
                    button.textContent = formattedDate;
                    button.setAttribute("data-date", isoDate);
                    button.onclick = function (event) {
                        event.preventDefault();
                        departureDateInput.value = isoDate;
                        dropdown.value = isoDate;
                        document.querySelectorAll("#departureOptions button").forEach(btn => btn.classList.remove("active"));
                        button.classList.add("active");
                    };
                    departureOptions.appendChild(button);

                    let option = document.createElement("option");
                    option.value = isoDate;
                    option.textContent = formattedDate;
                    dropdown.appendChild(option);
                });

                if (dates.length > 0) {
                    departureDateInput.value = dates[0];
                    dropdown.value = dates[0];
                    document.querySelector("#departureOptions button").classList.add("active");
                }
            }

            function onDropdownChange() {
                const dropdown = document.getElementById("customDepartureDate");
                const departureDateInput = document.getElementById("departureDate");
                departureDateInput.value = dropdown.value;
                document.querySelectorAll("#departureOptions button").forEach(btn => {
                    btn.classList.toggle("active", btn.getAttribute("data-date") === dropdown.value);
                });
            }

            function formatDateToDDMM(dateStr) {
                let date = new Date(dateStr);
                let day = date.getUTCDate();
                let month = date.getUTCMonth() + 1;
                let year = date.getUTCFullYear();
                return `${day}/${month}/${year}`;
            }

        </script>

