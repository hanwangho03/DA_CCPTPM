                        <path d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L2 8.207V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V8.207l.646.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293zM13 7.207V13.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V7.207l5-5z" />
                    </svg>
                    <a itemprop="item" href="@Url.Action("Index", "Tours")">
                        <i class="fa fa-home"></i> <span itemprop="name">@_localizer["Trang chủ"] / </span>
                    </a>
                    <meta itemprop="position" content="1">
                </li>

                <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem" class="active hidden-xs">
                    <a itemprop="item" class="link-detail-value" href="@Url.Action("Details", "Tours", new { id = Model.IdTour })">
                        <span itemprop="name">@Model.Name </span> <!-- Hiển thị tên tour -->
                    </a>
                    <meta itemprop="position" content="3">
                </li>
                <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem" class="active hidden-xs">

                </li>
            </ol>
        </section>

        <h1 class="tour-title">@Model.Name</h1>
        <div class="comment-count">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chat-dots" viewBox="0 0 16 16">
                <path d="M5 8a1 1 0 1 1-2 0 1 1 0 0 1 2 0m4 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0m3 1a1 1 0 1 0 0-2 1 1 0 0 0 0 2" />
                <path d="m2.165 15.803.02-.004c1.83-.363 2.948-.842 3.468-1.105A9 9 0 0 0 8 15c4.418 0 8-3.134 8-7s-3.582-7-8-7-8 3.134-8 7c0 1.76.743 3.37 1.97 4.6a10.4 10.4 0 0 1-.524 2.318l-.003.011a11 11 0 0 1-.244.637c-.079.186.074.394.273.362a22 22 0 0 0 .693-.125m.8-3.108a1 1 0 0 0-.287-.801C1.618 10.83 1 9.468 1 8c0-3.192 3.004-6 7-6s7 2.808 7 6-3.004 6-7 6a8 8 0 0 1-2.088-.272 1 1 0 0 0-.711.074c-.387.196-1.24.57-2.634.893a11 11 0 0 0 .398-2" />
            </svg> @Model.Comments.Count()<strong> @_localizer["đánh giá"]<br /></strong>
        </div>
        &nbsp;
        @if (Model.images != null && Model.images.Any())
        {
            var imageList = Model.images.ToList(); // Chuyển đổi ICollection<Image> thành List<Image>

            @*  <div class="tour-info-container" style="max-width: 800px; margin: auto; padding: 20px; border: 1px solid #ddd; border-radius: 10px;"> *@
            <div class="header" style="display: flex; justify-content: space-between; align-items: center; padding: 10px 0;">
                <div class="limited-stock" style="font-size: 14px; display: flex; align-items: center;">
                    <span class="badge badge-danger" style="margin-right: 10px;">@_localizer["Số Lượng Có Hạn"]</span>

                </div>
                <div class="tour-price" style="text-align: right; font-size: 18px;">
                    <div class="tour-duration" style="font-size: 16px; color: #ff5050;">Tour 5 Ngày</div>

                </div>
            </div>

            <!-- Carousel for tour images -->
            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    @for (int i = 0; i < imageList.Count; i++)
                    {
                        <li data-target="#carouselExampleIndicators" data-slide-to="@i" class="@(i == 0 ? "active" : "")"></li>
                    }
                </ol>

                <!-- Carousel items -->
                <div class="carousel-inner">
                    @for (int i = 0; i < imageList.Count; i++)
                    {
                        var imagePath = Url.Content("~/images/" + imageList[i].ImageUrl);
                        <div class="carousel-item @(i == 0 ? "active" : "")">
                            <img src="@imagePath" class="d-block w-100 img-fluid" alt="Image @i" style="max-height: 400px; object-fit: cover; width: 100%;">
                        </div>
                    }
                </div>

                <!-- Controls -->
                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>

            <!-- List of small images below the carousel -->
            <div class="image-thumbnails" style="display: flex; justify-content: center; margin-top: 15px;">
                @for (int i = 0; i < imageList.Count; i++)
                {
                    var imagePath = Url.Content("~/images/" + imageList[i].ImageUrl);
                    <img src="@imagePath" class="thumbnail" data-target="#carouselExampleIndicators" data-slide-to="@i"
                         style="width: 80px; height: 80px; object-fit: cover; margin: 0 5px; cursor: pointer; border: 2px solid transparent; transition: border 0.3s;">
                }
            </div>

            <!-- Tour Information -->

            @* </div> *@



        }
        else
        {
            <p>Không có hình ảnh nào cho tour này.</p>
        }


        <div class="owl-single dots-absolute owl-carousel">
            <img src="~/images/slider-1.jpg" alt="Free HTML Template by Untree.co" class="img-fluid rounded-20">
            <img src="~/images/slider-2.jpg" alt="Free HTML Template by Untree.co" class="img-fluid rounded-20">
            <img src="~/images/slider-3.jpg" alt="Free HTML Template by Untree.co" class="img-fluid rounded-20">
            <img src="~/images/slider-4.jpg" alt="Free HTML Template by Untree.co" class="img-fluid rounded-20">
            <img src="~/images/slider-5.jpg" alt="Free HTML Template by Untree.co" class="img-fluid rounded-20">
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="tour-info">
                    <!-- Phần trên chứa tiêu đề và dòng phân cách -->
                    <div class="tour-info-header" style="text-align: left;">
                        <h5>@_localizer["Tour Trọn Gói bao gồm"]</h5>
                        <p style="border-bottom: 1px solid #ccc; margin: 10px 0;"></p>
                    </div>

                    <!-- Phần dưới chia thành 3 cột -->
                    <div class="tour-info-body" style="display: flex; justify-content: space-between; gap: 20px;">
                        <!-- Cột 1 -->
                        <div style="flex: 1; font-size: 16px; color: #333;">
                            <p>✔ @_localizer["Vé máy bay"]</p>
                            <p>✔ @_localizer["Khách sạn"] 3-4*</p>
                        </div>

                        <!-- Cột 2 -->
                        <div style="flex: 1; font-size: 16px; color: #333;">
                            <p>✔ @_localizer["Bữa ăn"]</p>
                            <p>✔ @_localizer["Vé tham quan"]</p>
                        </div>

                        <!-- Cột 3 -->
                        <div style="flex: 1; font-size: 16px; color: #333;">
                            <p>✔ @_localizer["Hướng dẫn viên"]</p>
                            <p>✔ @_localizer["Bảo hiểm du lịch"]</p>
                        </div>
                    </div>
                </div>

                <div class="tour-info" style="text-align: left;">
                    <h3 style="margin-bottom: 10px;">@_localizer["Thông tin tour"]</h3>
                    <p style="border-bottom: 1px solid #ccc; margin: 10px 0;"></p>
                    <p style="margin: 5px 0;"><strong>@_localizer["Ngày khởi hành"]:</strong> @Model.StartDate</p>
                    <p style="margin: 5px 0;"><strong>@_localizer["Ngày kết thúc"]:</strong> @Model.EndDate</p>
                    <p style="margin: 5px 0;"><strong>@_localizer["Số ngày"]:</strong> @Model.MaxQuantity ngày</p>
                    <p style="margin: 5px 0;"><strong>@_localizer["Điểm khởi hành"]:</strong> Hà Nội</p>
                    <p style="margin: 5px 0;"><strong>@_localizer["Phương tiện"]:</strong> @Model.IdTransNavigation.Name</p>

                </div>

                <div class="tour-info" style="text-align: left;">
                    <h3>@_localizer["Trải nghiệm thú vị trong tour"]</h3>
                    <p style="border-bottom: 1px solid #ccc; margin: 10px 0;"></p>
                    <ul style="list-style-type: none; padding-left: 0;">
                        <li><p style="margin: 5px 0; padding: 0;">- @_localizer["Chiêm ngưỡng tượng Phật Vàng tại chùa Wat Traimit, biểu tượng tâm linh nổi tiếng."]</p></li>
                        <li><p style="margin: 5px 0; padding: 0;">- @_localizer["Khám phá Pattaya sôi động, thành phố biển không bao giờ ngủ."]</p></li>
                        <li><p style="margin: 5px 0; padding: 0;">- @_localizer["Hòa mình vào thế giới tiền sử đầy ấn tượng tại Công viên Khủng Long."]</p></li>
                        <li><p style="margin: 5px 0; padding: 0;">- @_localizer["Thư giãn trên bãi biển đảo Coral và tham gia các hoạt động thể thao dưới nước thú vị."]</p></li>
                        <li><p style="margin: 5px 0; padding: 0;">- @_localizer["Thưởng thức show diễn nghệ thuật hoành tráng, mang đậm văn hóa Thái Lan, và thỏa sức mua sắm tại các chợ đêm sầm uất."]</p></li>
                    </ul>
                </div>

                <div class="tour-details">
                    <h3>@_localizer["Lịch trình tour"]</h3>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
                        <img src="~/images/dalat.jpg" alt="Ngày 1" style="width: 120px; height: auto; border-radius: 5px;">
                        <div style="margin-left: 15px;">
                            <h5 style="font-size: 1.2em; font-weight: bold; margin-bottom: 5px;">Ngày 1: Hà Nội - Bangkok</h5>
                            <p style="font-size: 0.9em; color: #555;">@_localizer["Đi máy bay từ Hà Nội đến Bangkok"]</p>
                        </div>
                    </div>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
                        <img src="~/images/sapa.jpg" alt="Ngày 2" style="width: 120px; height: auto; border-radius: 5px;">
                        <div style="margin-left: 15px;">
                            <h5 style="font-size: 1.2em; font-weight: bold; margin-bottom: 5px;">Ngày 2: Bangkok</h5>
                            <p style="font-size: 0.9em; color: #555;">@_localizer["Tham quan Cung điện Vương quốc, Chùa Phật Ngọc"]</p>
                        </div>
                    </div>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
                        <img src="~/images/thai_lan.jpg" alt="Ngày 3" style="width: 120px; height: auto; border-radius: 5px;">
                        <div style="margin-left: 15px;">
                            <h5 style="font-size: 1.2em; font-weight: bold; margin-bottom: 5px;">Ngày 3: Bangkok - Pattaya</h5>
                            <p style="font-size: 0.9em; color: #555;">@_localizer["Đi xe bus từ Bangkok đến Pattaya"]</p>
                        </div>
                    </div>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
                        <img src="~/images/th (4).jpg" alt="Ngày 4" style="width: 120px; height: auto; border-radius: 5px;">
                        <div style="margin-left: 15px;">
                            <h5 style="font-size: 1.2em; font-weight: bold; margin-bottom: 5px;">Ngày 4: Pattaya</h5>
                            <p style="font-size: 0.9em; color: #555;">@_localizer["Tham quan Công Viên Khủng Long, biển Pattaya"]</p>
                        </div>
                    </div>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
                        <img src="~/images/th (5).jpg" alt="Ngày 5" style="width: 120px; height: auto; border-radius: 5px;">
                        <div style="margin-left: 15px;">
                            <h5 style="font-size: 1.2em; font-weight: bold; margin-bottom: 5px;">Ngày 5: Pattaya - Bangkok</h5>
                            <p style="font-size: 0.9em; color: #555;">@_localizer["Đi xe bus từ Pattaya đến Bangkok"]</p>
                        </div>
                    </div>

                    <div style="display: flex; align-items: center; padding: 15px; background-color: #f9f9f9; border: 1px solid #ddd; border-radius: 8px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); margin-bottom: 15px;">
