        <script>
            function setRating(rating) {
                // Đặt giá trị rating đã chọn cho radio button
                document.querySelector(`input[name="Rating"][value="${rating}"]`).checked = true;

                // Đổi màu các ngôi sao tùy theo số sao đã chọn
                const stars = document.querySelectorAll('.star');
                stars.forEach(star => {
                    const index = parseInt(star.getAttribute('data-index'));
                    // Đổi màu các ngôi sao có chỉ số <= rating
                    star.style.color = index <= rating ? '#FFD700' : '#ccc';
                });
            }
        </script>

        <script>
            // Giá cơ bản từ Model
            const pricePerAdult = parseFloat("@Model.Price") || 0;
            const pricePerChild = pricePerAdult * 0.5;
            const pricePerInfant = pricePerAdult * 0.25;

            // Hàm tính toán tổng giá
            function calculateTotalPrice() {
                // Lấy giá trị số lượng từ input
                const quantityAdult = parseInt(document.getElementById("quantityAdult")?.value) || 0;
                const quantityChild = parseInt(document.getElementById("quantityChild")?.value) || 0;
                const quantityInfant = parseInt(document.getElementById("quantityInfant")?.value) || 0;

                // Tính toán giá cho từng loại
                const totalPriceAdult = quantityAdult * pricePerAdult;
                const totalPriceChild = quantityChild * pricePerChild;
                const totalPriceInfant = quantityInfant * pricePerInfant;

                // Tổng giá trị
                const totalPrice = totalPriceAdult + totalPriceChild + totalPriceInfant;

                // Hiển thị giá cộng thêm kế bên mỗi input
                document.getElementById("priceAdultDisplay").textContent = "+" + totalPriceAdult.toLocaleString() + " VND";
                document.getElementById("priceChildDisplay").textContent = "+" + totalPriceChild.toLocaleString() + " VND";
                document.getElementById("priceInfantDisplay").textContent = "+" + totalPriceInfant.toLocaleString() + " VND";

                // Cập nhật giá trị tổng trong input readonly và input hidden
                document.getElementById("totalPriceDisplay").value = totalPrice.toLocaleString() + " VND";
                document.getElementById("totalPrice").value = totalPrice; // Giá trị này sẽ được gửi lên server
            }

            // Gắn sự kiện cho các input khi DOM đã tải xong
            document.addEventListener("DOMContentLoaded", function () {
                const inputs = document.querySelectorAll("#quantityAdult, #quantityChild, #quantityInfant");
                inputs.forEach(input => input.addEventListener("input", calculateTotalPrice));
            });
        </script>


